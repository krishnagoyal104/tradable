div>\n    );\n  }\n  \n}\n\nconst mapStateToProps = (state) => {\n\treturn{\n\t\taccount: state.account,\n    orders: state.orders,\n    loading: state.loader\n\t};\n};\n\nexport default withRouter(connect(mapStateToProps)(DasboardAsset));",["65","66"],"/home/shankar/Desktop/Desktop Dump/krishna/blockchain/hackathon/tradable/src/App.js",[],"/home/shankar/Desktop/Desktop Dump/krishna/blockchain/hackathon/tradable/src/components/home.js",["67","68","69","70","71","72"],"/home/shankar/Desktop/Desktop Dump/krishna/blockchain/hackathon/tradable/src/components/create.js",[],"/home/shankar/Desktop/Desktop Dump/krishna/blockchain/hackathon/tradable/src/components/assetView.js",["73","74","75","76","77","78"],"/home/shankar/Desktop/Desktop Dump/krishna/blockchain/hackathon/tradable/src/utils/abi.js",[],"/home/shankar/Desktop/Desktop Dump/krishna/blockchain/hackathon/tradable/src/components/asset.js",["79"],"/home/shankar/Desktop/Desktop Dump/krishna/blockchain/hackathon/tradable/src/actions/tx.js",[],"/home/shankar/Desktop/Desktop Dump/krishna/blockchain/hackathon/tradable/src/utils/index.js",["80","81","82"],{"ruleId":"83","severity":1,"message":"84","line":3,"column":1,"nodeType":"85","endLine":12,"endColumn":2},{"ruleId":"83","severity":1,"message":"84","line":31,"column":1,"nodeType":"85","endLine":36,"endColumn":3},{"ruleId":"86","severity":1,"message":"87","line":1,"column":20,"nodeType":"88","messageId":"89","endLine":1,"endColumn":35},{"ruleId":"86","severity":1,"message":"90","line":2,"column":9,"nodeType":"88","messageId":"89","endLine":2,"endColumn":16},{"ruleId":"91","severity":1,"message":"92","line":28,"column":29,"nodeType":"93","messageId":"94","endLine":28,"endColumn":31},{"ruleId":"86","severity":1,"message":"95","line":5,"column":9,"nodeType":"88","messageId":"89","endLine":5,"endColumn":16},{"ruleId":"96","severity":1,"message":"97","line":70,"column":29,"nodeType":"98","messageId":"99","endLine":70,"endColumn":31},{"ruleId":"100","severity":1,"message":"